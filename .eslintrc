{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react",
    "plugin:cypress/recommended"
  ],
  "plugins": ["jsx-a11y", "prettier", "cypress"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": true
      }
    ]
  },
  "env": {
    "cypress/globals": true
  }
}
